<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.get_profile_image %></td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', book_path(@user.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= render 'books/newbook', user: @user %>