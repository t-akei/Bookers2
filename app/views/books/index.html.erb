<h1>Books#index</h1>
<h1>Books</h1>

<% @books.each do |book| %>
  <P>Title</P>
  <span><%= book.title %></span>
<% end %>

<h1>User info</h1>
 











<h1>New book</h1>

<%= form_with model: Book.new do |f| %>

  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Opinion</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>

<% end %>